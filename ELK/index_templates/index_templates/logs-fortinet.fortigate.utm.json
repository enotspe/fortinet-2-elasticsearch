{
  "priority": 200,
  "template": {
    "lifecycle": {
      "enabled": true,
      "data_retention": "180d"
    }
  },
  "index_patterns": [
    "logs-fortinet.fortigate.utm*"
  ],
  "data_stream": {
  },
  "composed_of": [
    "ecs-base",
    "ecs-user",
    "ecs-observer",
    "ecs-destination",
    "ecs-source-modified",
    "ecs-network",
    "ecs-url",
    "ecs-rule",
    "ecs-data_stream",
    "ecs-organization",
    "ecs-ecs",
    "ecs-host",
    "ecs-http",
    "ecs-user_agent",
    "ecs-dns",
    "ecs-file-modified",
    "ecs-error",
    "ecs-tls-modified",
    "logs-fortinet.fortigate.utm@ilm",
    "strings_as_keyword@mappings",
    "auto_expand_replicas@settings",
    "refresh_interval@settings",
    "logs-fortinet.fortigate@mappings",
    "ecs-event-modified",
    "ecs-log-modified",
    "synthetic_source@mappings"
  ]
}
