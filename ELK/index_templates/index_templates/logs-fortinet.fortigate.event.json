{
  "priority": 200,
  "template": {
    "lifecycle": {
      "enabled": true,
      "data_retention": "180d"
    },
    "settings": {
        "index.mode": "logsdb",
        "mapping": {
          "ignore_malformed": "true"
        }
     }
  },
  "index_patterns": [
    "logs-fortinet.fortigate.event*"
  ],
  "data_stream": {
  },
  "composed_of": [
    "ecs-base",
    "ecs-user",
    "ecs-user_agent",
    "ecs-observer",
    "ecs-destination",
    "ecs-source",
    "ecs-network",
    "ecs-error",
    "ecs-url",
    "ecs-rule",
    "ecs-data_stream",
    "ecs-organization",
    "ecs-ecs",
    "logs-fortinet.fortigate.event@ilm",
    "strings_as_keyword@mappings",
    "auto_expand_replicas@settings",
    "refresh_interval@settings",
    "fortigate_event_7_6",
    "ecs-log",
    "ecs-event",
    "ecs-file"
  ]
}
