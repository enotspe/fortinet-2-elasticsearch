{
  "processors": [
    {
      "set": {
        "field": "event.ingested",
        "value": "{{_ingest.timestamp}}",
        "ignore_failure": true
      }
    },
    {
      "remove": {
        "field": [
          "host",
          "cloud",
          "agent"
        ],
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "grok": {
        "field": "message",
        "patterns": [
          "%{SYSLOG5424PRI}%{GREEDYDATA:message}"
        ],
        "pattern_definitions": {
          "SYSLOG5424PRI": "<%{NONNEGINT:log.syslog.priority:int}>"
        },
        "trace_match": true,
        "ignore_failure": true
      }
    },
    {
      "kv": {
        "field": "message",
        "field_split": "\\s(?=(?:[^\"]*\"[^\"]*\")*[^\"]*$)",
        "value_split": "=",
        "target_field": "fct",
        "strip_brackets": true,
        "ignore_failure": true
      }
    },
    {
      "set": {
        "field": "event.created",
        "ignore_failure": true,
        "copy_from": "@timestamp"
      }
    },
    {
      "date": {
        "field": "fct.timestamp",
        "formats": [
          "UNIX"
        ]
      }
    },
    {
      "set": {
        "field": "observer.serial_number",
        "copy_from": "fct.emsserial",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "observer.hostname",
        "copy_from": "fct.emshostname",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "observer.hostname",
        "copy_from": "fct.devname",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "agent.version",
        "copy_from": "fct.fctver",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "agent.serial_number",
        "copy_from": "fct.fctsn",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "host.ip",
        "copy_from": "fct.deviceip",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "host.mac",
        "copy_from": "fct.devicemac",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "error.code",
        "copy_from": "fct.error_code",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "host.hostname",
        "copy_from": "fct.hostname",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "host.domain",
        "copy_from": "fct.pcdomain",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "host.os.full",
        "copy_from": "fct.os",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "host.user.name",
        "copy_from": "fct.user",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "network.direction",
        "copy_from": "fct.direction",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "destination.address",
        "copy_from": "fct.dstip",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "destination.port",
        "copy_from": "fct.dstport",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "network.iana_number",
        "copy_from": "fct.proto",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "destination.bytes",
        "copy_from": "fct.rcvdbyte",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "destination.domain",
        "copy_from": "fct.remotename",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "url.domain",
        "copy_from": "fct.remotename",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "source.bytes",
        "copy_from": "fct.sentbyte",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "network.protocol",
        "copy_from": "fct.service",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "source.address",
        "copy_from": "fct.srcip",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "source.port",
        "copy_from": "fct.srcport",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "url.full",
        "copy_from": "fct.url",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "rule.name",
        "copy_from": "fct.usingpolicy",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "file.path",
        "copy_from": "fct.file",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "process.executable",
        "copy_from": "fct.srcname",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "event.id",
        "copy_from": "fct.id",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "log.level",
        "copy_from": "fct.level",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "event.timezone",
        "copy_from": "fct.tz",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "vulnerability.severity",
        "copy_from": "fct.vulnseverity",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "vulnerability.category",
        "copy_from": "fct.vulncat",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "vulnerability.id",
        "copy_from": "fct.vulnid",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "vulnerability.reference",
        "copy_from": "fct.vulnref",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "vulnerability.description",
        "copy_from": "fct.vulnname",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "process.executable",
        "copy_from": "fct.processname",
        "ignore_empty_value": true
      }
    },
    {
      "set": {
        "field": "session.id",
        "copy_from": "fct.sessionid",
        "ignore_empty_value": true
      }
    },
    {
      "grok": {
        "field": "source.address",
        "patterns": [
          "%{IP:source.ip}"
        ],
        "trace_match": true,
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "grok": {
        "field": "destination.address",
        "patterns": [
          "%{IP:destination.ip}"
        ],
        "trace_match": true,
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "registered_domain": {
        "field": "url.domain",
        "target_field": "url"
      }
    },
    {
      "append": {
        "field": "related.ip",
        "value": [
          "{{source.ip}}",
          "{{destination.ip}}",
          "{{host.ip}}"
        ]
      }
    },
    {
      "append": {
        "field": "related.user",
        "value": [
          "{{host.user.name}}"
        ]
      }
    },
    {
      "set": {
        "field": "_ingest.new_dataset",
        "value": "{{data_stream.dataset}}.{{fct.type}}"
      }
    },
    {
      "reroute": {
        "dataset": [
          "{{_ingest.new_dataset}}"
        ]
      }
    }
  ]
}
